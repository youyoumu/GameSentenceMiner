{
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2022",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "strict": true,
    "lib": ["ESNext"],
    "module": "ESNext",
    "noEmit": true,
    "moduleResolution": "NodeNext"
  }
}
